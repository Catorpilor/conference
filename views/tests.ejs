<form class="form-horizontal" method="post">
	<fieldset>
		<legend>预约会议</legend>
		<div class="control-group">
			<div class="controls">
                <div class="input-prepend">
                    <span class="add-on"><i class="icon-book"></i></span>
				    <input type="text" class="input-xlarge" id="confid" name="confid" placeholder="会议室号码">
                </div>
			</div>
        </div>
		<div class="control-group">
			<div class="controls">
                <div class="input-prepend">
                    <span class="add-on"><i class="icon-user"></i></span>
				    <input type="text" class="input-xlarge" id="confid" name="owner" value="<%= locals.user.username %>" disabled>
                </div>
			</div>
        </div>
        <div class="control-group">
            <div class="controls">
                <div class="input-prepend">
                    <span class="add-on"><i class="icon-user"></i></span>
                    <% include contactsl %>      
                </div>
            </div>
        </div>
        <hr>
		<div class="form-actions">
			<button type="submit" class="btn btn-primary" id="commit">提交</button>
		</div>
	</fieldset>
</form>
