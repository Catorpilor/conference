<% if ( stime  > ctime + 100000 ) { %> 
    <div class="mini-layout">
        <div class="mini-layout-body">
            <h5 class="text-info">抱歉，距离会议预约时间还有一段时间请稍候监控.</h5>
            <p>
                <input class="btn btn-primary" type="button" onClick="location.href='/'" value="返回">
            </p>
        </div>
    </div>
<% }else{ %>
    <p class="hide owner"><%= creater %></p>
    <p class="hide stimer"><%= stime %></p>
    <p class="hide status"><%= status %></p>
    <div class="row" id="framedisplay">
        <div class="col-md-3">
            <h4>当前会议室: <span class="label label-info" id="conf_id"><%= confid %></span></h4>
        </div>
        <div class="col-md-6 col-md-offset-3">
            <h4>
                <span class="strong">当前人数：</span>
                <span class="label label-default" id="curnum">0</span>
                &nbsp;
                <a href="#mymodal" role="button" class="btn add hide btn-info" data-toggle="modal">添加</a>
                <input type="button" class="btn hide btn-success" act="precord" id="arecord" value="暂停录音">
                <input type="button" class="btn hide btn-warning" act="srecord" id="record" value="开始录音">
            </h4>
        </div>
    </div>
    <hr/>
    <div class="row" id="userlist">

    </div>

    <div id="mymodal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-header">
            <h4 id="myModalLabel">添加与会人</h4>
        </div>
        <div class="modal-body">
            <div class="controls">
                <div class="input-prepend">
                    <span class="add-on"><i class="icon-user"></i></span>
                    <input type="text" class="input-xlarge" id="fordestnum" name="destnum" placeholder="请输入被叫号码">
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button data-dismiss="modal" class="btn">取消</button>
            <button class="btn btn-primary confirm">确定</button>
        </div>
    </div>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/javascripts/jquery.tmpl.min.js"></script>
    <script src="/javascripts/conf_io.js"></script>
<% } %>
